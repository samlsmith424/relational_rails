<%= form_with url: "/dealerships/#{@dealership.id}/cars", method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :body_style %>
<%= form.text_field :body_style %>
<%= form.label :mpg %>
<%= form.number_field :mpg %>
<%= form.label :electrical %>
<%= form.check_box :electrical %>
<%= form.submit "Create Car" %>
<% end %>
